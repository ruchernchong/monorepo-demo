{
  "name": "@ruchernchong/monorepo-app-a",
  "version": "1.2.0",
  "description": "App A: A serverless application leveraging @ruchernchong/monorepo-shared",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --env dev --port 8787",
    "deploy:staging": "wrangler deploy --env staging --minify",
    "deploy:prod": "wrangler deploy --env prod --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check-types": "tsc --noEmit"
  },
  "author": "Ru Chern Chong <hello@ruchern.dev>",
  "license": "MIT",
  "dependencies": {
    "@ruchernchong/monorepo-shared": "workspace:*",
    "hono": "^4.10.7",
    "sst": "3.17.23"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@cloudflare/workers-types": "4.20251120.0",
    "@vitest/coverage-v8": "^4.0.10",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10",
    "wrangler": "^4.50.0"
  }
}

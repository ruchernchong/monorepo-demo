{
  "name": "@ruchernchong/monorepo-web",
  "version": "1.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cf": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "preview": "pnpm run build:cf && opennextjs-cloudflare preview --port 3000",
    "deploy:preview": "pnpm build:cf && wrangler versions upload",
    "deploy:staging": "pnpm run build:cf && wrangler deploy --env staging",
    "deploy:prod": "pnpm run build:cf && wrangler deploy --env production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.13.1",
    "next": "16.1.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.59.1"
  }
}
